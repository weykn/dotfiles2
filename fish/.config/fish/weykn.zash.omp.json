{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "green",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "cyan",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <yellow>git(</>{{ .HEAD }}<yellow>)</>",
          "type": "git"
        },
        {
          "foreground": "#ff0000",
          "style": "plain",
          "template": " \uf119",
          "type": "status"
        },
        
    {
          "foreground": "cyan",
          "style": "plain",
          "template": " >",
          "type": "text"
    }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}